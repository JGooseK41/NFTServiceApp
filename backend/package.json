{
  "name": "nft-service-backend",
  "version": "1.0.0",
  "description": "Backend service for NFT legal notice delivery tracking",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node init-db.js",
    "migrate:ipfs": "node run-ipfs-migration.js",
    "migrate:server-fields": "node run-server-fields-migration.js",
    "migrate:compiled-fields": "node run-compiled-fields-migration.js",
    "migrate:agency": "node ensure-agency-columns.js",
    "migrate:all": "npm run migrate:ipfs && npm run migrate:server-fields && npm run migrate:compiled-fields && npm run migrate:agency",
    "fix-agency": "node fix-agency-column.js",
    "recover:ipfs": "node recover-from-ipfs.js",
    "recover:single": "node recover-from-ipfs.js --single"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "pdf-lib": "^1.17.1",
    "pdf2pic": "^3.1.3",
    "pg": "^8.11.3",
    "sharp": "^0.33.2",
    "tronweb": "^6.0.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "legal",
    "nft",
    "tron",
    "blockchain"
  ],
  "author": "",
  "license": "MIT"
}
